   <skos:Concept rdf:about="NGMP_ThematicCode_Fauna">
      <skos:altLabel>Fauna</skos:altLabel>
      <skos:prefLabel xml:lang="en">Fauna</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts which relate to animal organisms.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_Flora">
      <skos:altLabel>Flora</skos:altLabel>
      <skos:prefLabel xml:lang="en">Flora</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts which relate to members of the Plant Kingdom.</skos:scopeNote>
