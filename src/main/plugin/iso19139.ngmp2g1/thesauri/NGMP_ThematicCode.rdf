      <skos:prefLabel xml:lang="en">Operations and/or Events</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts which relate to certain operations or special eventsfor military or security purposes.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_WeatherPhenomena">
      <skos:altLabel>WeatherPhenomena</skos:altLabel>
      <skos:prefLabel xml:lang="en">Weather Phenomena</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts describing relatively stable weather phenomenas like wind conditions.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_ClimateConditions">
