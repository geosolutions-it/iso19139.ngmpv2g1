      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_AridAreas">
      <skos:altLabel>AridAreas</skos:altLabel>
      <skos:prefLabel xml:lang="en">Arid Areas</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts which describe very dry regions.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_RegionsRestrictedAreas">
      <skos:altLabel>RegionsRestrictedAreas</skos:altLabel>
