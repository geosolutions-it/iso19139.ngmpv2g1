   </skos:Concept>
   <skos:Concept rdf:about="NGMP_ThematicCode_LandSurveyRealEstate">
      <skos:altLabel>LandSurveyRealEstate</skos:altLabel>
      <skos:prefLabel xml:lang="en">Land-survey and/or Real Estate</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts which are used to designate official or legal properties abd/or which are used for surveying purposes.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
