      <skos:prefLabel xml:lang="en">Climate Zones and/or Regions</skos:prefLabel>
      <skos:scopeNote xml:lang="en">This Subgroup consists of Concepts describing climate zones and/or regions with special climate conditions.</skos:scopeNote>
      <skos:inScheme rdf:resource="http://www.dgiwg.org/ngmp/NGMP_ThematicCode"/>
